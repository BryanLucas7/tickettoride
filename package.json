{
  "name": "tickettoride",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install": "npm install --ignore-scripts && npm run postinstall",
    "postinstall": "python -m pip install fastapi uvicorn python-multipart",
    "dev": "concurrently --success first --restart-tries 3 \"cd frontend && next dev\" \"cd backend && uvicorn app.api:app --host 0.0.0.0 --port 8000 --reload\"",
    "dev:frontend": "cd frontend && next dev",
    "dev:backend": "cd backend && uvicorn app.api:app --host 0.0.0.0 --port 8000 --reload",
    "build": "cd frontend && next build",
    "start": "cd frontend && next start",
    "lint": "cd frontend && next lint"
  },
  "dependencies": {
    "@eslint/js": "9.37.0",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slot": "1.1.1",
    "axios": "1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "9.37.0",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-react-refresh": "0.4.23",
    "geist": "^1.3.1",
    "globals": "16.4.0",
    "lucide-react": "^0.454.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "1.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.9",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.2.1",
    "eslint-config-next": "16.0.1",
    "postcss": "^8.5",
    "tailwindcss": "^4.1.9",
    "typescript": "^5"
  }
}
